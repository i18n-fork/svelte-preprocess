{
  "name": "@3-/svelte-preprocess",
  "version": "5.0.20",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "description": "A Svelte preprocessor wrapper with baked-in support for commonly used preprocessors",
  "author": "Christian Kaisermann <christian@kaisermann.me>",
  "repository": "https://github.com/i18n-fork/svelte-preprocess",
  "keywords": [
    "svelte",
    "preprocess",
    "less",
    "stylus",
    "sass",
    "scss",
    "pug",
    "coffeescript"
  ],
  "engines": {
    "node": ">= 14.10.0"
  },
  "volta": {
    "node": "14.19.2"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "node scripts.js rmrf ./dist",
    "build": "tsc --build tsconfig.build.json",
    "dev": "pnpm build -w",
    "test": "jest",
    "lint": "eslint --ext js,ts .",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "postinstall": "echo \"[svelte-preprocess] Don't forget to install the preprocessors packages that will be used: sass, stylus, less, postcss & postcss-load-config, coffeescript, pug, etc...\"",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@3-/jsext": "^0.0.7",
    "@3-/loader": "^0.0.2",
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.25.4",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@kiwi/eslint-config": "^2.4.0",
    "@kiwi/prettier-config": "^2.4.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/stylus": "^0.48.42",
    "autoprefixer": "^10.4.20",
    "babel-minify": "^0.5.2",
    "coffeescript": "^2.7.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.10.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.38",
    "postcss-easy-import": "^4.0.0",
    "postcss-load-config": "^6.0.0",
    "prettier": "^3.3.3",
    "pug": "^3.0.3",
    "sass": "^1.77.4",
    "sugarss": "^4.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@3-/coffee_plus": "^0.1.3",
    "@types/pug": "^2.0.10",
    "detect-indent": "^6.1.0",
    "magic-string": "^0.30.11",
    "sorcery": "^1.0.0",
    "strip-indent": "^3.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.25.2",
    "coffeescript": "^2.7.0",
    "less": "^4.2.0",
    "postcss": "^8",
    "postcss-load-config": "^6.0.1",
    "pug": "^3.0.3",
    "sass": "^1.78.0",
    "stylus": "^0.63.0",
    "sugarss": "^4.0.1",
    "svelte": "^4.2.19",
    "typescript": "^5.6.2"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "coffeescript": {
      "optional": true
    },
    "less": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "postcss-load-config": {
      "optional": true
    },
    "pug": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "stylus": {
      "optional": true
    },
    "sugarss": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}